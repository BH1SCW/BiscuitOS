################################################
#
# init_nodemask_of_node
#
################################################
PK_INIT_NODEMASK_OF_NODE_NAME        := init_nodemask_of_node
PK_INIT_NODEMASK_OF_NODE_VERSION     := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_VERSION)
PK_INIT_NODEMASK_OF_NODE_SITE        := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_URL)
PK_INIT_NODEMASK_OF_NODE_GITHUB      := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_GITHUB)
PK_INIT_NODEMASK_OF_NODE_SUBNAME     :=
PK_INIT_NODEMASK_OF_NODE_BUILDARCH   := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_BUILDARCH)
PK_INIT_NODEMASK_OF_NODE_DIR         := $(srctree)/package/nodemask/init_nodemask_of_node
PK_INIT_NODEMASK_OF_NODE_PATCH       := $(PK_INIT_NODEMASK_OF_NODE_DIR)/patch
PK_INIT_NODEMASK_OF_NODE_BSBIT       := init_nodemask_of_node-$(PK_INIT_NODEMASK_OF_NODE_VERSION).bsbit
PK_INIT_NODEMASK_OF_NODE_OUT         :=
PK_INIT_NODEMASK_OF_NODE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_INIT_NODEMASK_OF_NODE_TAR         := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_TAR)
PK_INIT_NODEMASK_OF_NODE_PACKAGE     :=
PK_INIT_NODEMASK_OF_NODE_CONFIG      := $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CONFIG)
SUB_TARGET                  += $(PK_INIT_NODEMASK_OF_NODE_DIR)/build

PK_INIT_NODEMASK_OF_NODE_sh = $(CONFIG_SHELL) $(PK_INIT_NODEMASK_OF_NODE_SCRIPTS) \
                 $1X \
                 $(PK_INIT_NODEMASK_OF_NODE_NAME)X \
                 $(PK_INIT_NODEMASK_OF_NODE_VERSION)X \
                 $(PK_INIT_NODEMASK_OF_NODE_SITE)X \
                 $(PK_INIT_NODEMASK_OF_NODE_GITHUB)X \
                 $(PK_INIT_NODEMASK_OF_NODE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_INIT_NODEMASK_OF_NODE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_INIT_NODEMASK_OF_NODE_SUBNAME)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CONFIG)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_LIBPATH)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CPPFLAGS)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_DPKCONFIG)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CFLAGS)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_LDFLAGS)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CXXFLAGS)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_ASFLAGS)X \
                 $(PK_INIT_NODEMASK_OF_NODE_BSBIT)X \
                 $(PK_INIT_NODEMASK_OF_NODE_BUILDARCH)X \
		 $(PK_INIT_NODEMASK_OF_NODE_CSRCFILE)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_SRCFILE)X \
                 $(CONFIG_PK_INIT_NODEMASK_OF_NODE_CSRCFILE)

$(PK_INIT_NODEMASK_OF_NODE_DIR)/build: FORCE
	$(Q)$(call PK_INIT_NODEMASK_OF_NODE_sh,$(srctree))
