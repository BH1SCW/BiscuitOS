################################################
#
# nodes_intersects
#
################################################
PK_NODES_INTERSECTS_NAME        := nodes_intersects
PK_NODES_INTERSECTS_VERSION     := $(CONFIG_PK_NODES_INTERSECTS_VERSION)
PK_NODES_INTERSECTS_SITE        := $(CONFIG_PK_NODES_INTERSECTS_URL)
PK_NODES_INTERSECTS_GITHUB      := $(CONFIG_PK_NODES_INTERSECTS_GITHUB)
PK_NODES_INTERSECTS_SUBNAME     :=
PK_NODES_INTERSECTS_BUILDARCH   := $(CONFIG_PK_NODES_INTERSECTS_BUILDARCH)
PK_NODES_INTERSECTS_DIR         := $(srctree)/package/nodemask/nodes_intersects
PK_NODES_INTERSECTS_PATCH       := $(PK_NODES_INTERSECTS_DIR)/patch
PK_NODES_INTERSECTS_BSBIT       := nodes_intersects-$(PK_NODES_INTERSECTS_VERSION).bsbit
PK_NODES_INTERSECTS_OUT         :=
PK_NODES_INTERSECTS_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_NODES_INTERSECTS_TAR         := $(CONFIG_PK_NODES_INTERSECTS_TAR)
PK_NODES_INTERSECTS_PACKAGE     :=
PK_NODES_INTERSECTS_CONFIG      := $(CONFIG_PK_NODES_INTERSECTS_CONFIG)
SUB_TARGET                  += $(PK_NODES_INTERSECTS_DIR)/build

PK_NODES_INTERSECTS_sh = $(CONFIG_SHELL) $(PK_NODES_INTERSECTS_SCRIPTS) \
                 $1X \
                 $(PK_NODES_INTERSECTS_NAME)X \
                 $(PK_NODES_INTERSECTS_VERSION)X \
                 $(PK_NODES_INTERSECTS_SITE)X \
                 $(PK_NODES_INTERSECTS_GITHUB)X \
                 $(PK_NODES_INTERSECTS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_NODES_INTERSECTS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_NODES_INTERSECTS_SUBNAME)X \
                 $(CONFIG_PK_NODES_INTERSECTS_CONFIG)X \
                 $(CONFIG_PK_NODES_INTERSECTS_LIBPATH)X \
                 $(CONFIG_PK_NODES_INTERSECTS_CPPFLAGS)X \
                 $(CONFIG_PK_NODES_INTERSECTS_DPKCONFIG)X \
                 $(CONFIG_PK_NODES_INTERSECTS_CFLAGS)X \
                 $(CONFIG_PK_NODES_INTERSECTS_LDFLAGS)X \
                 $(CONFIG_PK_NODES_INTERSECTS_CXXFLAGS)X \
                 $(CONFIG_PK_NODES_INTERSECTS_ASFLAGS)X \
                 $(PK_NODES_INTERSECTS_BSBIT)X \
                 $(PK_NODES_INTERSECTS_BUILDARCH)X \
		 $(PK_NODES_INTERSECTS_CSRCFILE)X \
                 $(CONFIG_PK_NODES_INTERSECTS_SRCFILE)X \
                 $(CONFIG_PK_NODES_INTERSECTS_CSRCFILE)

$(PK_NODES_INTERSECTS_DIR)/build: FORCE
	$(Q)$(call PK_NODES_INTERSECTS_sh,$(srctree))
