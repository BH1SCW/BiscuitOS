# MSR-IA32_MTRRCAP
#
################################################
PK_MSR_IA32_MTRRCAP_NAME        := MSR-IA32_MTRRCAP
PK_MSR_IA32_MTRRCAP_VERSION     := $(CONFIG_PK_MSR_IA32_MTRRCAP_VERSION)
PK_MSR_IA32_MTRRCAP_SITE        := $(CONFIG_PK_MSR_IA32_MTRRCAP_URL)
PK_MSR_IA32_MTRRCAP_GITHUB      := $(CONFIG_PK_MSR_IA32_MTRRCAP_GITHUB)
PK_MSR_IA32_MTRRCAP_SUBNAME     :=
PK_MSR_IA32_MTRRCAP_BUILDARCH   := $(CONFIG_PK_MSR_IA32_MTRRCAP_BUILDARCH)
PK_MSR_IA32_MTRRCAP_DIR         := $(srctree)/package/Register/x86/MSR/MSR-IA32_MTRRCAP
PK_MSR_IA32_MTRRCAP_PATCH       := $(PK_MSR_IA32_MTRRCAP_DIR)/patch
PK_MSR_IA32_MTRRCAP_BSBIT       := MSR-IA32_MTRRCAP-$(PK_MSR_IA32_MTRRCAP_VERSION).bsbit
PK_MSR_IA32_MTRRCAP_OUT         :=
PK_MSR_IA32_MTRRCAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MSR_IA32_MTRRCAP_TAR         := $(CONFIG_PK_MSR_IA32_MTRRCAP_TAR)
PK_MSR_IA32_MTRRCAP_PACKAGE     :=
PK_MSR_IA32_MTRRCAP_CONFIG      := $(CONFIG_PK_MSR_IA32_MTRRCAP_CONFIG)
SUB_TARGET                  += $(PK_MSR_IA32_MTRRCAP_DIR)/build

PK_MSR_IA32_MTRRCAP_sh = $(CONFIG_SHELL) $(PK_MSR_IA32_MTRRCAP_SCRIPTS) \
                 $1X \
                 $(PK_MSR_IA32_MTRRCAP_NAME)X \
                 $(PK_MSR_IA32_MTRRCAP_VERSION)X \
                 $(PK_MSR_IA32_MTRRCAP_SITE)X \
                 $(PK_MSR_IA32_MTRRCAP_GITHUB)X \
                 $(PK_MSR_IA32_MTRRCAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MSR_IA32_MTRRCAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MSR_IA32_MTRRCAP_SUBNAME)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_CONFIG)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_LIBPATH)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_CPPFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_DPKCONFIG)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_CFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_LDFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_CXXFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_ASFLAGS)X \
                 $(PK_MSR_IA32_MTRRCAP_BSBIT)X \
                 $(PK_MSR_IA32_MTRRCAP_BUILDARCH)X \
		 $(PK_MSR_IA32_MTRRCAP_CSRCFILE)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_SRCFILE)X \
                 $(CONFIG_PK_MSR_IA32_MTRRCAP_CSRCFILE)

$(PK_MSR_IA32_MTRRCAP_DIR)/build: FORCE
	$(Q)$(call PK_MSR_IA32_MTRRCAP_sh,$(srctree))
