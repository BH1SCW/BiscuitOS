################################################
#
# MSR-IA32_MTRR_DEF_TYPE
#
################################################
PK_MSR_IA32_MTRR_DEF_TYPE_NAME        := MSR-IA32_MTRR_DEF_TYPE
PK_MSR_IA32_MTRR_DEF_TYPE_VERSION     := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_VERSION)
PK_MSR_IA32_MTRR_DEF_TYPE_SITE        := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_URL)
PK_MSR_IA32_MTRR_DEF_TYPE_GITHUB      := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_GITHUB)
PK_MSR_IA32_MTRR_DEF_TYPE_SUBNAME     :=
PK_MSR_IA32_MTRR_DEF_TYPE_BUILDARCH   := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_BUILDARCH)
PK_MSR_IA32_MTRR_DEF_TYPE_DIR         := $(srctree)/package/Register/x86/MSR/MSR-IA32_MTRR_DEF_TYPE
PK_MSR_IA32_MTRR_DEF_TYPE_PATCH       := $(PK_MSR_IA32_MTRR_DEF_TYPE_DIR)/patch
PK_MSR_IA32_MTRR_DEF_TYPE_BSBIT       := MSR-IA32_MTRR_DEF_TYPE-$(PK_MSR_IA32_MTRR_DEF_TYPE_VERSION).bsbit
PK_MSR_IA32_MTRR_DEF_TYPE_OUT         :=
PK_MSR_IA32_MTRR_DEF_TYPE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MSR_IA32_MTRR_DEF_TYPE_TAR         := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_TAR)
PK_MSR_IA32_MTRR_DEF_TYPE_PACKAGE     :=
PK_MSR_IA32_MTRR_DEF_TYPE_CONFIG      := $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CONFIG)
SUB_TARGET                  += $(PK_MSR_IA32_MTRR_DEF_TYPE_DIR)/build

PK_MSR_IA32_MTRR_DEF_TYPE_sh = $(CONFIG_SHELL) $(PK_MSR_IA32_MTRR_DEF_TYPE_SCRIPTS) \
                 $1X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_NAME)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_VERSION)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_SITE)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_GITHUB)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_SUBNAME)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CONFIG)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_LIBPATH)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CPPFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_DPKCONFIG)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_LDFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CXXFLAGS)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_ASFLAGS)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_BSBIT)X \
                 $(PK_MSR_IA32_MTRR_DEF_TYPE_BUILDARCH)X \
		 $(PK_MSR_IA32_MTRR_DEF_TYPE_CSRCFILE)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_SRCFILE)X \
                 $(CONFIG_PK_MSR_IA32_MTRR_DEF_TYPE_CSRCFILE)

$(PK_MSR_IA32_MTRR_DEF_TYPE_DIR)/build: FORCE
	$(Q)$(call PK_MSR_IA32_MTRR_DEF_TYPE_sh,$(srctree))
