################################################
#
# X86-Paging-32bit-PKMAP
#
################################################
PK_PAGING_32BIT_PKMAP_NAME        := X86-Paging-32bit-PKMAP
PK_PAGING_32BIT_PKMAP_VERSION     := $(CONFIG_PK_PAGING_32BIT_PKMAP_VERSION)
PK_PAGING_32BIT_PKMAP_SITE        := $(CONFIG_PK_PAGING_32BIT_PKMAP_URL)
PK_PAGING_32BIT_PKMAP_GITHUB      := $(CONFIG_PK_PAGING_32BIT_PKMAP_GITHUB)
PK_PAGING_32BIT_PKMAP_SUBNAME     :=
PK_PAGING_32BIT_PKMAP_BUILDARCH   := $(CONFIG_PK_PAGING_32BIT_PKMAP_BUILDARCH)
PK_PAGING_32BIT_PKMAP_DIR         := $(srctree)/package/paging/Paging-mode-32bit/Paging-32bit-PKMAP
PK_PAGING_32BIT_PKMAP_PATCH       := $(PK_PAGING_32BIT_PKMAP_DIR)/patch
PK_PAGING_32BIT_PKMAP_BSBIT       := X86-Paging-32bit-PKMAP-$(PK_PAGING_32BIT_PKMAP_VERSION).bsbit
PK_PAGING_32BIT_PKMAP_OUT         :=
PK_PAGING_32BIT_PKMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_32BIT_PKMAP_TAR         := $(CONFIG_PK_PAGING_32BIT_PKMAP_TAR)
PK_PAGING_32BIT_PKMAP_PACKAGE     :=
PK_PAGING_32BIT_PKMAP_CONFIG      := $(CONFIG_PK_PAGING_32BIT_PKMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_32BIT_PKMAP_DIR)/build

PK_PAGING_32BIT_PKMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_32BIT_PKMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_32BIT_PKMAP_NAME)X \
                 $(PK_PAGING_32BIT_PKMAP_VERSION)X \
                 $(PK_PAGING_32BIT_PKMAP_SITE)X \
                 $(PK_PAGING_32BIT_PKMAP_GITHUB)X \
                 $(PK_PAGING_32BIT_PKMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_32BIT_PKMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_32BIT_PKMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_ASFLAGS)X \
                 $(PK_PAGING_32BIT_PKMAP_BSBIT)X \
                 $(PK_PAGING_32BIT_PKMAP_BUILDARCH)X \
		 $(PK_PAGING_32BIT_PKMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_PKMAP_CSRCFILE)

$(PK_PAGING_32BIT_PKMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_32BIT_PKMAP_sh,$(srctree))
