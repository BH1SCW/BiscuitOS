################################################
#
# X86-Paging-32bit-Linear
#
################################################
PK_PAGING_32BIT_LINEAR_NAME        := X86-Paging-32bit-Linear
PK_PAGING_32BIT_LINEAR_VERSION     := $(CONFIG_PK_PAGING_32BIT_LINEAR_VERSION)
PK_PAGING_32BIT_LINEAR_SITE        := $(CONFIG_PK_PAGING_32BIT_LINEAR_URL)
PK_PAGING_32BIT_LINEAR_GITHUB      := $(CONFIG_PK_PAGING_32BIT_LINEAR_GITHUB)
PK_PAGING_32BIT_LINEAR_SUBNAME     :=
PK_PAGING_32BIT_LINEAR_BUILDARCH   := $(CONFIG_PK_PAGING_32BIT_LINEAR_BUILDARCH)
PK_PAGING_32BIT_LINEAR_DIR         := $(srctree)/package/paging/Paging-mode-32bit/Paging-32bit-Linear
PK_PAGING_32BIT_LINEAR_PATCH       := $(PK_PAGING_32BIT_LINEAR_DIR)/patch
PK_PAGING_32BIT_LINEAR_BSBIT       := X86-Paging-32bit-Linear-$(PK_PAGING_32BIT_LINEAR_VERSION).bsbit
PK_PAGING_32BIT_LINEAR_OUT         :=
PK_PAGING_32BIT_LINEAR_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_32BIT_LINEAR_TAR         := $(CONFIG_PK_PAGING_32BIT_LINEAR_TAR)
PK_PAGING_32BIT_LINEAR_PACKAGE     :=
PK_PAGING_32BIT_LINEAR_CONFIG      := $(CONFIG_PK_PAGING_32BIT_LINEAR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_32BIT_LINEAR_DIR)/build

PK_PAGING_32BIT_LINEAR_sh = $(CONFIG_SHELL) $(PK_PAGING_32BIT_LINEAR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_32BIT_LINEAR_NAME)X \
                 $(PK_PAGING_32BIT_LINEAR_VERSION)X \
                 $(PK_PAGING_32BIT_LINEAR_SITE)X \
                 $(PK_PAGING_32BIT_LINEAR_GITHUB)X \
                 $(PK_PAGING_32BIT_LINEAR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_32BIT_LINEAR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_32BIT_LINEAR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_CONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_ASFLAGS)X \
                 $(PK_PAGING_32BIT_LINEAR_BSBIT)X \
                 $(PK_PAGING_32BIT_LINEAR_BUILDARCH)X \
		 $(PK_PAGING_32BIT_LINEAR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_LINEAR_CSRCFILE)

$(PK_PAGING_32BIT_LINEAR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_32BIT_LINEAR_sh,$(srctree))
