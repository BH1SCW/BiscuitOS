################################################
#
# X86-Paging-32bit-KMAP
#
################################################
PK_PAGING_32BIT_KMAP_NAME        := X86-Paging-32bit-KMAP
PK_PAGING_32BIT_KMAP_VERSION     := $(CONFIG_PK_PAGING_32BIT_KMAP_VERSION)
PK_PAGING_32BIT_KMAP_SITE        := $(CONFIG_PK_PAGING_32BIT_KMAP_URL)
PK_PAGING_32BIT_KMAP_GITHUB      := $(CONFIG_PK_PAGING_32BIT_KMAP_GITHUB)
PK_PAGING_32BIT_KMAP_SUBNAME     :=
PK_PAGING_32BIT_KMAP_BUILDARCH   := $(CONFIG_PK_PAGING_32BIT_KMAP_BUILDARCH)
PK_PAGING_32BIT_KMAP_DIR         := $(srctree)/package/paging/Paging-mode-32bit/Paging-32bit-KMAP
PK_PAGING_32BIT_KMAP_PATCH       := $(PK_PAGING_32BIT_KMAP_DIR)/patch
PK_PAGING_32BIT_KMAP_BSBIT       := X86-Paging-32bit-KMAP-$(PK_PAGING_32BIT_KMAP_VERSION).bsbit
PK_PAGING_32BIT_KMAP_OUT         :=
PK_PAGING_32BIT_KMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_32BIT_KMAP_TAR         := $(CONFIG_PK_PAGING_32BIT_KMAP_TAR)
PK_PAGING_32BIT_KMAP_PACKAGE     :=
PK_PAGING_32BIT_KMAP_CONFIG      := $(CONFIG_PK_PAGING_32BIT_KMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_32BIT_KMAP_DIR)/build

PK_PAGING_32BIT_KMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_32BIT_KMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_32BIT_KMAP_NAME)X \
                 $(PK_PAGING_32BIT_KMAP_VERSION)X \
                 $(PK_PAGING_32BIT_KMAP_SITE)X \
                 $(PK_PAGING_32BIT_KMAP_GITHUB)X \
                 $(PK_PAGING_32BIT_KMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_32BIT_KMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_32BIT_KMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_ASFLAGS)X \
                 $(PK_PAGING_32BIT_KMAP_BSBIT)X \
                 $(PK_PAGING_32BIT_KMAP_BUILDARCH)X \
		 $(PK_PAGING_32BIT_KMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_KMAP_CSRCFILE)

$(PK_PAGING_32BIT_KMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_32BIT_KMAP_sh,$(srctree))
