################################################
#
# BiscuitOS-SWAP-userspace
#
################################################
PK_SWAP_USERSPACE_NAME        := BiscuitOS-SWAP-userspace
PK_SWAP_USERSPACE_VERSION     := $(CONFIG_PK_SWAP_USERSPACE_VERSION)
PK_SWAP_USERSPACE_SITE        := $(CONFIG_PK_SWAP_USERSPACE_URL)
PK_SWAP_USERSPACE_GITHUB      := $(CONFIG_PK_SWAP_USERSPACE_GITHUB)
PK_SWAP_USERSPACE_SUBNAME     :=
PK_SWAP_USERSPACE_BUILDARCH   := $(CONFIG_PK_SWAP_USERSPACE_BUILDARCH)
PK_SWAP_USERSPACE_DIR         := $(srctree)/package/paging/BiscuitOS-SWAP-userspace
PK_SWAP_USERSPACE_PATCH       := $(PK_SWAP_USERSPACE_DIR)/patch
PK_SWAP_USERSPACE_BSBIT       := BiscuitOS-SWAP-userspace-$(PK_SWAP_USERSPACE_VERSION).bsbit
PK_SWAP_USERSPACE_OUT         :=
PK_SWAP_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SWAP_USERSPACE_TAR         := $(CONFIG_PK_SWAP_USERSPACE_TAR)
PK_SWAP_USERSPACE_PACKAGE     :=
PK_SWAP_USERSPACE_CONFIG      := $(CONFIG_PK_SWAP_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_SWAP_USERSPACE_DIR)/build

PK_SWAP_USERSPACE_sh = $(CONFIG_SHELL) $(PK_SWAP_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_SWAP_USERSPACE_NAME)X \
                 $(PK_SWAP_USERSPACE_VERSION)X \
                 $(PK_SWAP_USERSPACE_SITE)X \
                 $(PK_SWAP_USERSPACE_GITHUB)X \
                 $(PK_SWAP_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SWAP_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SWAP_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_SWAP_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_SWAP_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_SWAP_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_SWAP_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_SWAP_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_SWAP_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_SWAP_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_SWAP_USERSPACE_ASFLAGS)X \
                 $(PK_SWAP_USERSPACE_BSBIT)X \
                 $(PK_SWAP_USERSPACE_BUILDARCH)X \
		 $(PK_SWAP_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_SWAP_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_SWAP_USERSPACE_CSRCFILE)

$(PK_SWAP_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_SWAP_USERSPACE_sh,$(srctree))
