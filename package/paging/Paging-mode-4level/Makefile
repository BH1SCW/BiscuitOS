################################################
#
# X86-Paging-mode-4level
#
################################################
PK_PAGING_MODE_4LEVEL_NAME        := X86-Paging-mode-4level
PK_PAGING_MODE_4LEVEL_VERSION     := $(CONFIG_PK_PAGING_MODE_4LEVEL_VERSION)
PK_PAGING_MODE_4LEVEL_SITE        := $(CONFIG_PK_PAGING_MODE_4LEVEL_URL)
PK_PAGING_MODE_4LEVEL_GITHUB      := $(CONFIG_PK_PAGING_MODE_4LEVEL_GITHUB)
PK_PAGING_MODE_4LEVEL_SUBNAME     :=
PK_PAGING_MODE_4LEVEL_BUILDARCH   := $(CONFIG_PK_PAGING_MODE_4LEVEL_BUILDARCH)
PK_PAGING_MODE_4LEVEL_DIR         := $(srctree)/package/paging/X86-Paging-mode-4level
PK_PAGING_MODE_4LEVEL_PATCH       := $(PK_PAGING_MODE_4LEVEL_DIR)/patch
PK_PAGING_MODE_4LEVEL_BSBIT       := X86-Paging-mode-4level-$(PK_PAGING_MODE_4LEVEL_VERSION).bsbit
PK_PAGING_MODE_4LEVEL_OUT         :=
PK_PAGING_MODE_4LEVEL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_MODE_4LEVEL_TAR         := $(CONFIG_PK_PAGING_MODE_4LEVEL_TAR)
PK_PAGING_MODE_4LEVEL_PACKAGE     :=
PK_PAGING_MODE_4LEVEL_CONFIG      := $(CONFIG_PK_PAGING_MODE_4LEVEL_CONFIG)
SUB_TARGET                  += $(PK_PAGING_MODE_4LEVEL_DIR)/build

PK_PAGING_MODE_4LEVEL_sh = $(CONFIG_SHELL) $(PK_PAGING_MODE_4LEVEL_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_MODE_4LEVEL_NAME)X \
                 $(PK_PAGING_MODE_4LEVEL_VERSION)X \
                 $(PK_PAGING_MODE_4LEVEL_SITE)X \
                 $(PK_PAGING_MODE_4LEVEL_GITHUB)X \
                 $(PK_PAGING_MODE_4LEVEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_MODE_4LEVEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_MODE_4LEVEL_SUBNAME)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_CONFIG)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_LIBPATH)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_CFLAGS)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_ASFLAGS)X \
                 $(PK_PAGING_MODE_4LEVEL_BSBIT)X \
                 $(PK_PAGING_MODE_4LEVEL_BUILDARCH)X \
		 $(PK_PAGING_MODE_4LEVEL_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_SRCFILE)X \
                 $(CONFIG_PK_PAGING_MODE_4LEVEL_CSRCFILE)

$(PK_PAGING_MODE_4LEVEL_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_MODE_4LEVEL_sh,$(srctree))
