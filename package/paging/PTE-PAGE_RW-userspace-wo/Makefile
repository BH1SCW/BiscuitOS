PK_PTE_PAGE_RW_USERSPACE_WO_NAME        := PTE-PAGE_RW-userspace-wo
PK_PTE_PAGE_RW_USERSPACE_WO_VERSION     := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_VERSION)
PK_PTE_PAGE_RW_USERSPACE_WO_SITE        := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_URL)
PK_PTE_PAGE_RW_USERSPACE_WO_GITHUB      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_GITHUB)
PK_PTE_PAGE_RW_USERSPACE_WO_SUBNAME     :=
PK_PTE_PAGE_RW_USERSPACE_WO_BUILDARCH   := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_BUILDARCH)
PK_PTE_PAGE_RW_USERSPACE_WO_DIR         := $(srctree)/package/paging/PTE-PAGE_RW-userspace-wo
PK_PTE_PAGE_RW_USERSPACE_WO_PATCH       := $(PK_PTE_PAGE_RW_USERSPACE_WO_DIR)/patch
PK_PTE_PAGE_RW_USERSPACE_WO_BSBIT       := PTE-PAGE_RW-userspace-wo-$(PK_PTE_PAGE_RW_USERSPACE_WO_VERSION).bsbit
PK_PTE_PAGE_RW_USERSPACE_WO_OUT         :=
PK_PTE_PAGE_RW_USERSPACE_WO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PTE_PAGE_RW_USERSPACE_WO_TAR         := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_TAR)
PK_PTE_PAGE_RW_USERSPACE_WO_PACKAGE     :=
PK_PTE_PAGE_RW_USERSPACE_WO_CONFIG      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CONFIG)
SUB_TARGET                  += $(PK_PTE_PAGE_RW_USERSPACE_WO_DIR)/build

PK_PTE_PAGE_RW_USERSPACE_WO_sh = $(CONFIG_SHELL) $(PK_PTE_PAGE_RW_USERSPACE_WO_SCRIPTS) \
                 $1X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_VERSION)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_SITE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_GITHUB)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_SUBNAME)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_LIBPATH)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CPPFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_DPKCONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_LDFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CXXFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_ASFLAGS)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_BSBIT)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_WO_BUILDARCH)X \
		 $(PK_PTE_PAGE_RW_USERSPACE_WO_CSRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_SRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_WO_CSRCFILE)

$(PK_PTE_PAGE_RW_USERSPACE_WO_DIR)/build: FORCE
	$(Q)$(call PK_PTE_PAGE_RW_USERSPACE_WO_sh,$(srctree))
