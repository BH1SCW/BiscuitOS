PK_PTE_PAGE_ACCESSED_KERNEL_NAME        := PTE-PAGE_ACCESSED-kernel
PK_PTE_PAGE_ACCESSED_KERNEL_VERSION     := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_VERSION)
PK_PTE_PAGE_ACCESSED_KERNEL_SITE        := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_URL)
PK_PTE_PAGE_ACCESSED_KERNEL_GITHUB      := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_GITHUB)
PK_PTE_PAGE_ACCESSED_KERNEL_SUBNAME     :=
PK_PTE_PAGE_ACCESSED_KERNEL_BUILDARCH   := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_BUILDARCH)
PK_PTE_PAGE_ACCESSED_KERNEL_DIR         := $(srctree)/package/paging/PTE-PAGE_ACCESSED-kernel
PK_PTE_PAGE_ACCESSED_KERNEL_PATCH       := $(PK_PTE_PAGE_ACCESSED_KERNEL_DIR)/patch
PK_PTE_PAGE_ACCESSED_KERNEL_BSBIT       := PTE-PAGE_ACCESSED-kernel-$(PK_PTE_PAGE_ACCESSED_KERNEL_VERSION).bsbit
PK_PTE_PAGE_ACCESSED_KERNEL_OUT         :=
PK_PTE_PAGE_ACCESSED_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PTE_PAGE_ACCESSED_KERNEL_TAR         := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_TAR)
PK_PTE_PAGE_ACCESSED_KERNEL_PACKAGE     :=
PK_PTE_PAGE_ACCESSED_KERNEL_CONFIG      := $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_PTE_PAGE_ACCESSED_KERNEL_DIR)/build

PK_PTE_PAGE_ACCESSED_KERNEL_sh = $(CONFIG_SHELL) $(PK_PTE_PAGE_ACCESSED_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_NAME)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_VERSION)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_SITE)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_GITHUB)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_ASFLAGS)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_BSBIT)X \
                 $(PK_PTE_PAGE_ACCESSED_KERNEL_BUILDARCH)X \
		 $(PK_PTE_PAGE_ACCESSED_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_KERNEL_CSRCFILE)

$(PK_PTE_PAGE_ACCESSED_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_PTE_PAGE_ACCESSED_KERNEL_sh,$(srctree))
