PK_PTE_PAGE_ACCESSED_USERSPACE_NAME        := PTE-PAGE_ACCESSED-userspace
PK_PTE_PAGE_ACCESSED_USERSPACE_VERSION     := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_VERSION)
PK_PTE_PAGE_ACCESSED_USERSPACE_SITE        := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_URL)
PK_PTE_PAGE_ACCESSED_USERSPACE_GITHUB      := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_GITHUB)
PK_PTE_PAGE_ACCESSED_USERSPACE_SUBNAME     :=
PK_PTE_PAGE_ACCESSED_USERSPACE_BUILDARCH   := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_BUILDARCH)
PK_PTE_PAGE_ACCESSED_USERSPACE_DIR         := $(srctree)/package/paging/PTE-PAGE_ACCESSED-userspace
PK_PTE_PAGE_ACCESSED_USERSPACE_PATCH       := $(PK_PTE_PAGE_ACCESSED_USERSPACE_DIR)/patch
PK_PTE_PAGE_ACCESSED_USERSPACE_BSBIT       := PTE-PAGE_ACCESSED-userspace-$(PK_PTE_PAGE_ACCESSED_USERSPACE_VERSION).bsbit
PK_PTE_PAGE_ACCESSED_USERSPACE_OUT         :=
PK_PTE_PAGE_ACCESSED_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PTE_PAGE_ACCESSED_USERSPACE_TAR         := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_TAR)
PK_PTE_PAGE_ACCESSED_USERSPACE_PACKAGE     :=
PK_PTE_PAGE_ACCESSED_USERSPACE_CONFIG      := $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_PTE_PAGE_ACCESSED_USERSPACE_DIR)/build

PK_PTE_PAGE_ACCESSED_USERSPACE_sh = $(CONFIG_SHELL) $(PK_PTE_PAGE_ACCESSED_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_NAME)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_VERSION)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_SITE)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_GITHUB)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_ASFLAGS)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_BSBIT)X \
                 $(PK_PTE_PAGE_ACCESSED_USERSPACE_BUILDARCH)X \
		 $(PK_PTE_PAGE_ACCESSED_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_ACCESSED_USERSPACE_CSRCFILE)

$(PK_PTE_PAGE_ACCESSED_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_PTE_PAGE_ACCESSED_USERSPACE_sh,$(srctree))
