PK_PTE_PAGE_RW_USERSPACE_RW_NAME        := PTE-PAGE_RW-userspace-rw
PK_PTE_PAGE_RW_USERSPACE_RW_VERSION     := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_VERSION)
PK_PTE_PAGE_RW_USERSPACE_RW_SITE        := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_URL)
PK_PTE_PAGE_RW_USERSPACE_RW_GITHUB      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_GITHUB)
PK_PTE_PAGE_RW_USERSPACE_RW_SUBNAME     :=
PK_PTE_PAGE_RW_USERSPACE_RW_BUILDARCH   := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_BUILDARCH)
PK_PTE_PAGE_RW_USERSPACE_RW_DIR         := $(srctree)/package/paging/PTE-PAGE_RW-userspace-rw
PK_PTE_PAGE_RW_USERSPACE_RW_PATCH       := $(PK_PTE_PAGE_RW_USERSPACE_RW_DIR)/patch
PK_PTE_PAGE_RW_USERSPACE_RW_BSBIT       := PTE-PAGE_RW-userspace-rw-$(PK_PTE_PAGE_RW_USERSPACE_RW_VERSION).bsbit
PK_PTE_PAGE_RW_USERSPACE_RW_OUT         :=
PK_PTE_PAGE_RW_USERSPACE_RW_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PTE_PAGE_RW_USERSPACE_RW_TAR         := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_TAR)
PK_PTE_PAGE_RW_USERSPACE_RW_PACKAGE     :=
PK_PTE_PAGE_RW_USERSPACE_RW_CONFIG      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CONFIG)
SUB_TARGET                  += $(PK_PTE_PAGE_RW_USERSPACE_RW_DIR)/build

PK_PTE_PAGE_RW_USERSPACE_RW_sh = $(CONFIG_SHELL) $(PK_PTE_PAGE_RW_USERSPACE_RW_SCRIPTS) \
                 $1X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_VERSION)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_SITE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_GITHUB)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_SUBNAME)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_LIBPATH)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CPPFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_DPKCONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_LDFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CXXFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_ASFLAGS)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_BSBIT)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RW_BUILDARCH)X \
		 $(PK_PTE_PAGE_RW_USERSPACE_RW_CSRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_SRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RW_CSRCFILE)

$(PK_PTE_PAGE_RW_USERSPACE_RW_DIR)/build: FORCE
	$(Q)$(call PK_PTE_PAGE_RW_USERSPACE_RW_sh,$(srctree))
