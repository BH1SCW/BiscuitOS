################################################
#
# BiscuitOS-Compact-userspace
#
################################################
PK_COMPACT_USERSPACE_NAME        := BiscuitOS-Compact-userspace
PK_COMPACT_USERSPACE_VERSION     := $(CONFIG_PK_COMPACT_USERSPACE_VERSION)
PK_COMPACT_USERSPACE_SITE        := $(CONFIG_PK_COMPACT_USERSPACE_URL)
PK_COMPACT_USERSPACE_GITHUB      := $(CONFIG_PK_COMPACT_USERSPACE_GITHUB)
PK_COMPACT_USERSPACE_SUBNAME     :=
PK_COMPACT_USERSPACE_BUILDARCH   := $(CONFIG_PK_COMPACT_USERSPACE_BUILDARCH)
PK_COMPACT_USERSPACE_DIR         := $(srctree)/package/paging/BiscuitOS-Compact-userspace
PK_COMPACT_USERSPACE_PATCH       := $(PK_COMPACT_USERSPACE_DIR)/patch
PK_COMPACT_USERSPACE_BSBIT       := BiscuitOS-Compact-userspace-$(PK_COMPACT_USERSPACE_VERSION).bsbit
PK_COMPACT_USERSPACE_OUT         :=
PK_COMPACT_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_COMPACT_USERSPACE_TAR         := $(CONFIG_PK_COMPACT_USERSPACE_TAR)
PK_COMPACT_USERSPACE_PACKAGE     :=
PK_COMPACT_USERSPACE_CONFIG      := $(CONFIG_PK_COMPACT_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_COMPACT_USERSPACE_DIR)/build

PK_COMPACT_USERSPACE_sh = $(CONFIG_SHELL) $(PK_COMPACT_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_COMPACT_USERSPACE_NAME)X \
                 $(PK_COMPACT_USERSPACE_VERSION)X \
                 $(PK_COMPACT_USERSPACE_SITE)X \
                 $(PK_COMPACT_USERSPACE_GITHUB)X \
                 $(PK_COMPACT_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_COMPACT_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_COMPACT_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_ASFLAGS)X \
                 $(PK_COMPACT_USERSPACE_BSBIT)X \
                 $(PK_COMPACT_USERSPACE_BUILDARCH)X \
		 $(PK_COMPACT_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_COMPACT_USERSPACE_CSRCFILE)

$(PK_COMPACT_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_COMPACT_USERSPACE_sh,$(srctree))
