PK_PTE_PAGE_RW_USERSPACE_RO_NAME        := PTE-PAGE_RW-userspace-ro
PK_PTE_PAGE_RW_USERSPACE_RO_VERSION     := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_VERSION)
PK_PTE_PAGE_RW_USERSPACE_RO_SITE        := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_URL)
PK_PTE_PAGE_RW_USERSPACE_RO_GITHUB      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_GITHUB)
PK_PTE_PAGE_RW_USERSPACE_RO_SUBNAME     :=
PK_PTE_PAGE_RW_USERSPACE_RO_BUILDARCH   := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_BUILDARCH)
PK_PTE_PAGE_RW_USERSPACE_RO_DIR         := $(srctree)/package/paging/PTE-PAGE_RW-userspace-ro
PK_PTE_PAGE_RW_USERSPACE_RO_PATCH       := $(PK_PTE_PAGE_RW_USERSPACE_RO_DIR)/patch
PK_PTE_PAGE_RW_USERSPACE_RO_BSBIT       := PTE-PAGE_RW-userspace-ro-$(PK_PTE_PAGE_RW_USERSPACE_RO_VERSION).bsbit
PK_PTE_PAGE_RW_USERSPACE_RO_OUT         :=
PK_PTE_PAGE_RW_USERSPACE_RO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PTE_PAGE_RW_USERSPACE_RO_TAR         := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_TAR)
PK_PTE_PAGE_RW_USERSPACE_RO_PACKAGE     :=
PK_PTE_PAGE_RW_USERSPACE_RO_CONFIG      := $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CONFIG)
SUB_TARGET                  += $(PK_PTE_PAGE_RW_USERSPACE_RO_DIR)/build

PK_PTE_PAGE_RW_USERSPACE_RO_sh = $(CONFIG_SHELL) $(PK_PTE_PAGE_RW_USERSPACE_RO_SCRIPTS) \
                 $1X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_VERSION)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_SITE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_GITHUB)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_SUBNAME)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_LIBPATH)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CPPFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_DPKCONFIG)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_LDFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CXXFLAGS)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_ASFLAGS)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_BSBIT)X \
                 $(PK_PTE_PAGE_RW_USERSPACE_RO_BUILDARCH)X \
		 $(PK_PTE_PAGE_RW_USERSPACE_RO_CSRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_SRCFILE)X \
                 $(CONFIG_PK_PTE_PAGE_RW_USERSPACE_RO_CSRCFILE)

$(PK_PTE_PAGE_RW_USERSPACE_RO_DIR)/build: FORCE
	$(Q)$(call PK_PTE_PAGE_RW_USERSPACE_RO_sh,$(srctree))
