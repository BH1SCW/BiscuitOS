################################################
#
# BiscuitOS-hugetlb-file-share-mapping-posix
#
################################################
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_NAME        := BiscuitOS-hugetlb-file-share-mapping-posix
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_VERSION)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_URL)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_GITHUB)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SUBNAME     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_BUILDARCH)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-file-share-mapping-posix
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_PATCH       := $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_DIR)/patch
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_BSBIT       := BiscuitOS-hugetlb-file-share-mapping-posix-$(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_VERSION).bsbit
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_OUT         :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_TAR)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_PACKAGE     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_DIR)/build

PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_FILE_SHARE_POSIX_sh,$(srctree))
