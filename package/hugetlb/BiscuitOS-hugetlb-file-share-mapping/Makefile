################################################
#
# BiscuitOS-hugetlb-file-share-mapping
#
################################################
PK_BISCUITOS_HUGETLB_FILE_SHARE_NAME        := BiscuitOS-hugetlb-file-share-mapping
PK_BISCUITOS_HUGETLB_FILE_SHARE_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_VERSION)
PK_BISCUITOS_HUGETLB_FILE_SHARE_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_URL)
PK_BISCUITOS_HUGETLB_FILE_SHARE_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_GITHUB)
PK_BISCUITOS_HUGETLB_FILE_SHARE_SUBNAME     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_BUILDARCH)
PK_BISCUITOS_HUGETLB_FILE_SHARE_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-file-share-mapping
PK_BISCUITOS_HUGETLB_FILE_SHARE_PATCH       := $(PK_BISCUITOS_HUGETLB_FILE_SHARE_DIR)/patch
PK_BISCUITOS_HUGETLB_FILE_SHARE_BSBIT       := BiscuitOS-hugetlb-file-share-mapping-$(PK_BISCUITOS_HUGETLB_FILE_SHARE_VERSION).bsbit
PK_BISCUITOS_HUGETLB_FILE_SHARE_OUT         :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_FILE_SHARE_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_TAR)
PK_BISCUITOS_HUGETLB_FILE_SHARE_PACKAGE     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_FILE_SHARE_DIR)/build

PK_BISCUITOS_HUGETLB_FILE_SHARE_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_FILE_SHARE_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_FILE_SHARE_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_FILE_SHARE_sh,$(srctree))
