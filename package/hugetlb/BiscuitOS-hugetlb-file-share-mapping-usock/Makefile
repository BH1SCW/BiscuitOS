PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_NAME        := BiscuitOS-hugetlb-file-share-mapping-usock
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_VERSION)
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_URL)
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_GITHUB)
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SUBNAME     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_BUILDARCH)
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-file-share-mapping-usock
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_PATCH       := $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_DIR)/patch
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_BSBIT       := BiscuitOS-hugetlb-file-share-mapping-usock-$(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_VERSION).bsbit
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_OUT         :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_TAR)
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_PACKAGE     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_DIR)/build

PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_FILE_SHARE_USOCK_sh,$(srctree))
