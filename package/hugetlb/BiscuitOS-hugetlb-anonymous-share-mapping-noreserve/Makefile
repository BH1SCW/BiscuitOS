PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_NAME        := BiscuitOS-hugetlb-anonymous-share-mapping-noreserve
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_VERSION)
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_URL)
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_GITHUB)
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SUBNAME     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_BUILDARCH)
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-anonymous-share-mapping-noreserve
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_PATCH       := $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_DIR)/patch
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_BSBIT       := BiscuitOS-hugetlb-anonymous-share-mapping-noreserve-$(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_VERSION).bsbit
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_OUT         :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_TAR)
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_PACKAGE     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_DIR)/build

PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_ANON_SHARE_NORSVD_sh,$(srctree))
