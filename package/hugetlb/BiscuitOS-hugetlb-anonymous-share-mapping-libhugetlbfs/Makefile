PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_NAME        := BiscuitOS-hugetlb-anonymous-share-mapping-libhugetlbfs
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_VERSION)
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_URL)
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_GITHUB)
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SUBNAME     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_BUILDARCH)
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-anonymous-share-mapping-libhugetlbfs
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_PATCH       := $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_DIR)/patch
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_BSBIT       := BiscuitOS-hugetlb-anonymous-share-mapping-libhugetlbfs-$(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_VERSION).bsbit
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_OUT         :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_TAR)
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_PACKAGE     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_DIR)/build

PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_ANON_SHARE_LIBHUGE_sh,$(srctree))
