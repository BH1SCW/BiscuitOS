PK_HUGETLB_HISTORY_V2_6_6_BSBIT       := BiscuitOS-hugetlb-history-v2.6.6-$(PK_HUGETLB_HISTORY_V2_6_6_VERSION).bsbit
PK_HUGETLB_HISTORY_V2_6_6_OUT         :=
