PK_HUGETLB_HISTORY_V2_6_6_TAR         := $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_TAR)
PK_HUGETLB_HISTORY_V2_6_6_PACKAGE     :=
PK_HUGETLB_HISTORY_V2_6_6_CONFIG      := $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_CONFIG)
SUB_TARGET                  += $(PK_HUGETLB_HISTORY_V2_6_6_DIR)/build

