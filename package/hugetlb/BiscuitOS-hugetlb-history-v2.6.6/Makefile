                 $(PK_HUGETLB_HISTORY_V2_6_6_SITE)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_GITHUB)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_SUBNAME)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_CONFIG)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_LIBPATH)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_CPPFLAGS)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_DPKCONFIG)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_CFLAGS)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_LDFLAGS)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_CXXFLAGS)X \
                 $(CONFIG_PK_HUGETLB_HISTORY_V2_6_6_ASFLAGS)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_BSBIT)X \
                 $(PK_HUGETLB_HISTORY_V2_6_6_BUILDARCH)X \
		 $(PK_HUGETLB_HISTORY_V2_6_6_CSRCFILE)X \
