################################################
#
# BiscuitOS-libhugetlbfs-usage
#
################################################
PK_LIBHUGETLBFS_USAGE_NAME        := BiscuitOS-libhugetlbfs-usage
PK_LIBHUGETLBFS_USAGE_VERSION     := $(CONFIG_PK_LIBHUGETLBFS_USAGE_VERSION)
PK_LIBHUGETLBFS_USAGE_SITE        := $(CONFIG_PK_LIBHUGETLBFS_USAGE_URL)
PK_LIBHUGETLBFS_USAGE_GITHUB      := $(CONFIG_PK_LIBHUGETLBFS_USAGE_GITHUB)
PK_LIBHUGETLBFS_USAGE_SUBNAME     :=
PK_LIBHUGETLBFS_USAGE_BUILDARCH   := $(CONFIG_PK_LIBHUGETLBFS_USAGE_BUILDARCH)
PK_LIBHUGETLBFS_USAGE_DIR         := $(srctree)/package/hugetlb/BiscuitOS-libhugetlbfs-usage
PK_LIBHUGETLBFS_USAGE_PATCH       := $(PK_LIBHUGETLBFS_USAGE_DIR)/patch
PK_LIBHUGETLBFS_USAGE_BSBIT       := BiscuitOS-libhugetlbfs-usage-$(PK_LIBHUGETLBFS_USAGE_VERSION).bsbit
PK_LIBHUGETLBFS_USAGE_OUT         :=
PK_LIBHUGETLBFS_USAGE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_LIBHUGETLBFS_USAGE_TAR         := $(CONFIG_PK_LIBHUGETLBFS_USAGE_TAR)
PK_LIBHUGETLBFS_USAGE_PACKAGE     :=
PK_LIBHUGETLBFS_USAGE_CONFIG      := $(CONFIG_PK_LIBHUGETLBFS_USAGE_CONFIG)
SUB_TARGET                  += $(PK_LIBHUGETLBFS_USAGE_DIR)/build

PK_LIBHUGETLBFS_USAGE_sh = $(CONFIG_SHELL) $(PK_LIBHUGETLBFS_USAGE_SCRIPTS) \
                 $1X \
                 $(PK_LIBHUGETLBFS_USAGE_NAME)X \
                 $(PK_LIBHUGETLBFS_USAGE_VERSION)X \
                 $(PK_LIBHUGETLBFS_USAGE_SITE)X \
                 $(PK_LIBHUGETLBFS_USAGE_GITHUB)X \
                 $(PK_LIBHUGETLBFS_USAGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_LIBHUGETLBFS_USAGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_LIBHUGETLBFS_USAGE_SUBNAME)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_CONFIG)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_LIBPATH)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_CPPFLAGS)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_DPKCONFIG)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_CFLAGS)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_LDFLAGS)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_CXXFLAGS)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_ASFLAGS)X \
                 $(PK_LIBHUGETLBFS_USAGE_BSBIT)X \
                 $(PK_LIBHUGETLBFS_USAGE_BUILDARCH)X \
		 $(PK_LIBHUGETLBFS_USAGE_CSRCFILE)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_SRCFILE)X \
                 $(CONFIG_PK_LIBHUGETLBFS_USAGE_CSRCFILE)

$(PK_LIBHUGETLBFS_USAGE_DIR)/build: FORCE
	$(Q)$(call PK_LIBHUGETLBFS_USAGE_sh,$(srctree))
