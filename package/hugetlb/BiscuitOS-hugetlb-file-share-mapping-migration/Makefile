PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_NAME        := BiscuitOS-hugetlb-file-share-mapping-migration
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_VERSION)
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_URL)
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_GITHUB)
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SUBNAME     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_BUILDARCH)
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-file-share-mapping-migration
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_PATCH       := $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_DIR)/patch
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_BSBIT       := BiscuitOS-hugetlb-file-share-mapping-migration-$(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_VERSION).bsbit
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_OUT         :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_TAR)
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_PACKAGE     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_DIR)/build

PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_FILE_SHARE_MIGRATE_sh,$(srctree))
