PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_NAME        := BiscuitOS-hugetlb-anonymous-share-mapping-sysv
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_VERSION)
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_URL)
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_GITHUB)
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SUBNAME     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_BUILDARCH)
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-anonymous-share-mapping-sysv
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_PATCH       := $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_DIR)/patch
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_BSBIT       := BiscuitOS-hugetlb-anonymous-share-mapping-sysv-$(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_VERSION).bsbit
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_OUT         :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_TAR)
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_PACKAGE     :=
PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_DIR)/build

PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_ANON_SHARE_SYSV_sh,$(srctree))
