PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_NAME        := BiscuitOS-hugetlb-file-share-mapping-populate
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_VERSION     := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_VERSION)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SITE        := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_URL)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_GITHUB)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SUBNAME     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_BUILDARCH)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_DIR         := $(srctree)/package/hugetlb/BiscuitOS-hugetlb-file-share-mapping-populate
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_PATCH       := $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_DIR)/patch
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_BSBIT       := BiscuitOS-hugetlb-file-share-mapping-populate-$(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_VERSION).bsbit
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_OUT         :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_TAR         := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_TAR)
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_PACKAGE     :=
PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_DIR)/build

PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_VERSION)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SITE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_GITHUB)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_BSBIT)X \
                 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_CSRCFILE)

$(PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGETLB_FILE_SHARE_POPULATE_sh,$(srctree))
