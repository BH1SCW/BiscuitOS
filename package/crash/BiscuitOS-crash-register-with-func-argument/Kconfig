	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/Tools/Crash/BiscuitOS-crash-register-with-func-argument"
	string "url"

config PK_CRASH_FUNC_ARGS_GITHUB
	string "GitHub"

config PK_CRASH_FUNC_ARGS_SRCFILE
	default "main.c Kconfig Makefile"
	string "Source code list"

config PK_CRASH_FUNC_ARGS_CSRCFILE
	string "Compile source code list"

config PK_CRASH_FUNC_ARGS_CONFIG
	string "CONFIG: Project Configure Options"

config PK_CRASH_FUNC_ARGS_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_CRASH_FUNC_ARGS_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_CRASH_FUNC_ARGS_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_CRASH_FUNC_ARGS_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_CRASH_FUNC_ARGS_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_CRASH_FUNC_ARGS_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_CRASH_FUNC_ARGS_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_CRASH_FUNC_ARGS_BUILDARCH
	string "--host options on configure for Architecture"

