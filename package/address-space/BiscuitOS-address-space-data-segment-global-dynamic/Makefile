PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_NAME        := BiscuitOS-address-space-data-segment-global-dynamic
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_VERSION     := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_VERSION)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SITE        := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_URL)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_GITHUB      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_GITHUB)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SUBNAME     :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_BUILDARCH   := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_BUILDARCH)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_DIR         := $(srctree)/package/address-space/BiscuitOS-address-space-data-segment-global-dynamic
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_PATCH       := $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_DIR)/patch
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_BSBIT       := BiscuitOS-address-space-data-segment-global-dynamic-$(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_VERSION).bsbit
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_OUT         :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_TAR         := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_TAR)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_PACKAGE     :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CONFIG      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_DIR)/build

PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_VERSION)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SITE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_GITHUB)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_ASFLAGS)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_BSBIT)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_BUILDARCH)X \
		 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_CSRCFILE)

$(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_AS_DATA_SEGMENT_GL_DL_sh,$(srctree))
