PK_BISCUITOS_AS_DATA_SEGMENT_GL_NAME        := BiscuitOS-address-space-data-segment-global
PK_BISCUITOS_AS_DATA_SEGMENT_GL_VERSION     := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_VERSION)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_SITE        := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_URL)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_GITHUB      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_GITHUB)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_SUBNAME     :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_BUILDARCH   := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_BUILDARCH)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_DIR         := $(srctree)/package/address-space/BiscuitOS-address-space-data-segment-global
PK_BISCUITOS_AS_DATA_SEGMENT_GL_PATCH       := $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DIR)/patch
PK_BISCUITOS_AS_DATA_SEGMENT_GL_BSBIT       := BiscuitOS-address-space-data-segment-global-$(PK_BISCUITOS_AS_DATA_SEGMENT_GL_VERSION).bsbit
PK_BISCUITOS_AS_DATA_SEGMENT_GL_OUT         :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_AS_DATA_SEGMENT_GL_TAR         := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_TAR)
PK_BISCUITOS_AS_DATA_SEGMENT_GL_PACKAGE     :=
PK_BISCUITOS_AS_DATA_SEGMENT_GL_CONFIG      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DIR)/build

PK_BISCUITOS_AS_DATA_SEGMENT_GL_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_VERSION)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_SITE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_GITHUB)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_ASFLAGS)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_BSBIT)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_BUILDARCH)X \
		 $(PK_BISCUITOS_AS_DATA_SEGMENT_GL_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GL_CSRCFILE)

$(PK_BISCUITOS_AS_DATA_SEGMENT_GL_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_AS_DATA_SEGMENT_GL_sh,$(srctree))
