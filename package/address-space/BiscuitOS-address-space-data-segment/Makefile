################################################
#
# BiscuitOS-address-space-data-segment
#
################################################
PK_BISCUITOS_AS_DATA_SEGMENT_NAME        := BiscuitOS-address-space-data-segment
PK_BISCUITOS_AS_DATA_SEGMENT_VERSION     := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_VERSION)
PK_BISCUITOS_AS_DATA_SEGMENT_SITE        := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_URL)
PK_BISCUITOS_AS_DATA_SEGMENT_GITHUB      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_GITHUB)
PK_BISCUITOS_AS_DATA_SEGMENT_SUBNAME     :=
PK_BISCUITOS_AS_DATA_SEGMENT_BUILDARCH   := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_BUILDARCH)
PK_BISCUITOS_AS_DATA_SEGMENT_DIR         := $(srctree)/package/address-space/BiscuitOS-address-space-data-segment
PK_BISCUITOS_AS_DATA_SEGMENT_PATCH       := $(PK_BISCUITOS_AS_DATA_SEGMENT_DIR)/patch
PK_BISCUITOS_AS_DATA_SEGMENT_BSBIT       := BiscuitOS-address-space-data-segment-$(PK_BISCUITOS_AS_DATA_SEGMENT_VERSION).bsbit
PK_BISCUITOS_AS_DATA_SEGMENT_OUT         :=
PK_BISCUITOS_AS_DATA_SEGMENT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_AS_DATA_SEGMENT_TAR         := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_TAR)
PK_BISCUITOS_AS_DATA_SEGMENT_PACKAGE     :=
PK_BISCUITOS_AS_DATA_SEGMENT_CONFIG      := $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_AS_DATA_SEGMENT_DIR)/build

PK_BISCUITOS_AS_DATA_SEGMENT_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_AS_DATA_SEGMENT_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_VERSION)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_SITE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_GITHUB)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_ASFLAGS)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_BSBIT)X \
                 $(PK_BISCUITOS_AS_DATA_SEGMENT_BUILDARCH)X \
		 $(PK_BISCUITOS_AS_DATA_SEGMENT_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_AS_DATA_SEGMENT_CSRCFILE)

$(PK_BISCUITOS_AS_DATA_SEGMENT_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_AS_DATA_SEGMENT_sh,$(srctree))
