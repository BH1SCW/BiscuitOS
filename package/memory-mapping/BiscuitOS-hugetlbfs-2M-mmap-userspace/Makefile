################################################
#
# BiscuitOS-hugetlbfs-2M-mmap-userspace
#
################################################
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_NAME        := BiscuitOS-hugetlbfs-2M-mmap-userspace
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_VERSION     := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_VERSION)
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SITE        := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_URL)
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_GITHUB      := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_GITHUB)
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SUBNAME     :=
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_BUILDARCH   := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_BUILDARCH)
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-hugetlbfs-2M-mmap-userspace
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_PATCH       := $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_DIR)/patch
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_BSBIT       := BiscuitOS-hugetlbfs-2M-mmap-userspace-$(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_VERSION).bsbit
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_OUT         :=
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_TAR         := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_TAR)
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_PACKAGE     :=
PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CONFIG      := $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_DIR)/build

PK_BISCUITOS_HUGE2MMMAP_USERSPACE_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_NAME)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_VERSION)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SITE)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_GITHUB)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_ASFLAGS)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_BSBIT)X \
                 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_BUILDARCH)X \
		 $(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_HUGE2MMMAP_USERSPACE_CSRCFILE)

$(PK_BISCUITOS_HUGE2MMMAP_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_HUGE2MMMAP_USERSPACE_sh,$(srctree))
