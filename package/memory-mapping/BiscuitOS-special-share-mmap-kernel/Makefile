################################################
#
# BiscuitOS-special-share-mmap-kernel
#
################################################
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_NAME        := BiscuitOS-special-share-mmap-kernel
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_VERSION     := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_VERSION)
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SITE        := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_URL)
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_GITHUB      := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_GITHUB)
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SUBNAME     :=
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_BUILDARCH   := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_BUILDARCH)
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-special-share-mmap-kernel
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_PATCH       := $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_DIR)/patch
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_BSBIT       := BiscuitOS-special-share-mmap-kernel-$(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_VERSION).bsbit
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_OUT         :=
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_TAR         := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_TAR)
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_PACKAGE     :=
PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CONFIG      := $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_DIR)/build

PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_NAME)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_VERSION)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SITE)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_GITHUB)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_ASFLAGS)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_BSBIT)X \
                 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_BUILDARCH)X \
		 $(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_CSRCFILE)

$(PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_SPECIAL_SHARE_MMAP_KERNEL_sh,$(srctree))
