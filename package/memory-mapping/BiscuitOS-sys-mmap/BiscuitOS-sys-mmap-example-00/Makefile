PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_NAME        := BiscuitOS-sys-mmap-example-00
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_VERSION     := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_VERSION)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SITE        := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_URL)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_GITHUB      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_GITHUB)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SUBNAME     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_BUILDARCH   := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_BUILDARCH)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-sys-mmap/BiscuitOS-sys-mmap-example-00
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_PATCH       := $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_DIR)/patch
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_BSBIT       := BiscuitOS-sys-mmap-example-00-$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_VERSION).bsbit
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_OUT         :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_TAR         := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_TAR)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_PACKAGE     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CONFIG      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_DIR)/build

PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_VERSION)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SITE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_GITHUB)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_ASFLAGS)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_BSBIT)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_BUILDARCH)X \
		 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_CSRCFILE)

$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_SYS_MMAP_EXAMPLE_00_sh,$(srctree))
