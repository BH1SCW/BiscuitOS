PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_NAME        := BiscuitOS-sys-mmap-example-03
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_VERSION     := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_VERSION)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SITE        := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_URL)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_GITHUB      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_GITHUB)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SUBNAME     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_BUILDARCH   := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_BUILDARCH)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-sys-mmap/BiscuitOS-sys-mmap-example-03
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_PATCH       := $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_DIR)/patch
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_BSBIT       := BiscuitOS-sys-mmap-example-03-$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_VERSION).bsbit
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_OUT         :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_TAR         := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_TAR)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_PACKAGE     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CONFIG      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_DIR)/build

PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_VERSION)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SITE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_GITHUB)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_ASFLAGS)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_BSBIT)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_BUILDARCH)X \
		 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_CSRCFILE)

$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_SYS_MMAP_EXAMPLE_03_sh,$(srctree))
