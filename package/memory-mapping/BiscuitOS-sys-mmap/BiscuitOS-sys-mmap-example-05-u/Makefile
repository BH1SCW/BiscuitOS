SUB_TARGET                  += $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_05_U_DIR)/build

PK_BISCUITOS_SYS_MMAP_EXAMPLE_05_U_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_05_U_SCRIPTS) \
                 $1X \
