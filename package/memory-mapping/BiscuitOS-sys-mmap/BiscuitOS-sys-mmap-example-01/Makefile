################################################
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_NAME        := BiscuitOS-sys-mmap-example-01
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_VERSION     := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_VERSION)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SITE        := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_URL)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_GITHUB      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_GITHUB)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SUBNAME     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_BUILDARCH   := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_BUILDARCH)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-sys-mmap/BiscuitOS-sys-mmap-example-01
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_PATCH       := $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_DIR)/patch
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_BSBIT       := BiscuitOS-sys-mmap-example-01-$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_VERSION).bsbit
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_OUT         :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_TAR         := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_TAR)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_PACKAGE     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CONFIG      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_DIR)/build

PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_VERSION)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SITE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_GITHUB)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_ASFLAGS)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_BSBIT)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_BUILDARCH)X \
		 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_CSRCFILE)

$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_SYS_MMAP_EXAMPLE_01_sh,$(srctree))
