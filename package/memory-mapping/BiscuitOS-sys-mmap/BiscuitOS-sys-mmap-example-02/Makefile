PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_NAME        := BiscuitOS-sys-mmap-example-02
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_VERSION     := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_VERSION)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SITE        := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_URL)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_GITHUB      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_GITHUB)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SUBNAME     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_BUILDARCH   := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_BUILDARCH)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-sys-mmap/BiscuitOS-sys-mmap-example-02
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_PATCH       := $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_DIR)/patch
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_BSBIT       := BiscuitOS-sys-mmap-example-02-$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_VERSION).bsbit
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_OUT         :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_TAR         := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_TAR)
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_PACKAGE     :=
PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CONFIG      := $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_DIR)/build

PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_VERSION)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SITE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_GITHUB)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_ASFLAGS)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_BSBIT)X \
                 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_BUILDARCH)X \
		 $(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_CSRCFILE)

$(PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_SYS_MMAP_EXAMPLE_02_sh,$(srctree))
