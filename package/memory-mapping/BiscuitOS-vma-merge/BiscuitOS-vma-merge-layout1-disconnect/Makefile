PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_NAME        := BiscuitOS-vma-merge-layout1-disconnect
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_VERSION     := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_VERSION)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SITE        := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_URL)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_GITHUB      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_GITHUB)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SUBNAME     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_BUILDARCH   := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_BUILDARCH)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-vma-merge/BiscuitOS-vma-merge-layout1-disconnect
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_PATCH       := $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_DIR)/patch
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_BSBIT       := BiscuitOS-vma-merge-layout1-disconnect-$(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_VERSION).bsbit
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_OUT         :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_TAR         := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_TAR)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_PACKAGE     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CONFIG      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_DIR)/build

PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_VERSION)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SITE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_GITHUB)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_ASFLAGS)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_BSBIT)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_BUILDARCH)X \
		 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_CSRCFILE)

$(PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_VMA_MERGE_LAYOUT1_DISCONNECT_sh,$(srctree))
