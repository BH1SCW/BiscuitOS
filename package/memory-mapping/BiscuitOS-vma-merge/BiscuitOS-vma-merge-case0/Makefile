PK_BISCUITOS_VMA_MERGE_CASE0_SUBNAME     :=
PK_BISCUITOS_VMA_MERGE_CASE0_BUILDARCH   := $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_BUILDARCH)
PK_BISCUITOS_VMA_MERGE_CASE0_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-vma-merge/BiscuitOS-vma-merge-case0
PK_BISCUITOS_VMA_MERGE_CASE0_PATCH       := $(PK_BISCUITOS_VMA_MERGE_CASE0_DIR)/patch
PK_BISCUITOS_VMA_MERGE_CASE0_BSBIT       := BiscuitOS-vma-merge-case0-$(PK_BISCUITOS_VMA_MERGE_CASE0_VERSION).bsbit
PK_BISCUITOS_VMA_MERGE_CASE0_OUT         :=
PK_BISCUITOS_VMA_MERGE_CASE0_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_VMA_MERGE_CASE0_TAR         := $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_TAR)
PK_BISCUITOS_VMA_MERGE_CASE0_PACKAGE     :=
PK_BISCUITOS_VMA_MERGE_CASE0_CONFIG      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_VMA_MERGE_CASE0_DIR)/build

PK_BISCUITOS_VMA_MERGE_CASE0_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_VMA_MERGE_CASE0_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_NAME)X \
