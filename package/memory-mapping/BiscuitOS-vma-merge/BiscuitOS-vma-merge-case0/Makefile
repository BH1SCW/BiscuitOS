                 $(PK_BISCUITOS_VMA_MERGE_CASE0_SITE)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_GITHUB)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_ASFLAGS)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_BSBIT)X \
                 $(PK_BISCUITOS_VMA_MERGE_CASE0_BUILDARCH)X \
		 $(PK_BISCUITOS_VMA_MERGE_CASE0_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_CASE0_SRCFILE)X \
