PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_VERSION     := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_VERSION)
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SITE        := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_URL)
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_GITHUB      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_GITHUB)
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SUBNAME     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_BUILDARCH   := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_BUILDARCH)
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-vma-merge/BiscuitOS-vma-merge-layout0-intersect
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_PATCH       := $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_DIR)/patch
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_BSBIT       := BiscuitOS-vma-merge-layout0-intersect-$(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_VERSION).bsbit
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_OUT         :=
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_TAR         := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_TAR)
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_PACKAGE     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CONFIG      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_DIR)/build

PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_VERSION)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SITE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_GITHUB)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_ASFLAGS)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_BSBIT)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_BUILDARCH)X \
		 $(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_CSRCFILE)

$(PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_VMA_MERGE_LAYOUT0_INTERSECT_sh,$(srctree))
