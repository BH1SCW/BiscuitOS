PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_NAME        := BiscuitOS-vma-merge-layout1-adjoin
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_VERSION     := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_VERSION)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SITE        := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_URL)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_GITHUB      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_GITHUB)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SUBNAME     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_BUILDARCH   := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_BUILDARCH)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-vma-merge/BiscuitOS-vma-merge-layout1-adjoin
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_PATCH       := $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_DIR)/patch
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_BSBIT       := BiscuitOS-vma-merge-layout1-adjoin-$(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_VERSION).bsbit
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_OUT         :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_TAR         := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_TAR)
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_PACKAGE     :=
PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CONFIG      := $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_DIR)/build

PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_VERSION)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SITE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_GITHUB)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_ASFLAGS)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_BSBIT)X \
                 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_BUILDARCH)X \
		 $(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_CSRCFILE)

$(PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_VMA_MERGE_LAYOUT1_ADJOIN_sh,$(srctree))
