# BiscuitOS-file-interval-tree-fork
#
################################################
PK_INTERVAL_TREE_FILE_FORK_NAME        := BiscuitOS-file-interval-tree-fork
PK_INTERVAL_TREE_FILE_FORK_VERSION     := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_VERSION)
PK_INTERVAL_TREE_FILE_FORK_SITE        := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_URL)
PK_INTERVAL_TREE_FILE_FORK_GITHUB      := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_GITHUB)
PK_INTERVAL_TREE_FILE_FORK_SUBNAME     :=
PK_INTERVAL_TREE_FILE_FORK_BUILDARCH   := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_BUILDARCH)
PK_INTERVAL_TREE_FILE_FORK_DIR         := $(srctree)/package/interval-tree/BiscuitOS-file-interval-tree-fork
PK_INTERVAL_TREE_FILE_FORK_PATCH       := $(PK_INTERVAL_TREE_FILE_FORK_DIR)/patch
PK_INTERVAL_TREE_FILE_FORK_BSBIT       := BiscuitOS-file-interval-tree-fork-$(PK_INTERVAL_TREE_FILE_FORK_VERSION).bsbit
PK_INTERVAL_TREE_FILE_FORK_OUT         :=
PK_INTERVAL_TREE_FILE_FORK_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_INTERVAL_TREE_FILE_FORK_TAR         := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_TAR)
PK_INTERVAL_TREE_FILE_FORK_PACKAGE     :=
PK_INTERVAL_TREE_FILE_FORK_CONFIG      := $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CONFIG)
SUB_TARGET                  += $(PK_INTERVAL_TREE_FILE_FORK_DIR)/build

PK_INTERVAL_TREE_FILE_FORK_sh = $(CONFIG_SHELL) $(PK_INTERVAL_TREE_FILE_FORK_SCRIPTS) \
                 $1X \
                 $(PK_INTERVAL_TREE_FILE_FORK_NAME)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_VERSION)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_SITE)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_GITHUB)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_SUBNAME)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CONFIG)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_LIBPATH)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CPPFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_DPKCONFIG)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_LDFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CXXFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_ASFLAGS)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_BSBIT)X \
                 $(PK_INTERVAL_TREE_FILE_FORK_BUILDARCH)X \
		 $(PK_INTERVAL_TREE_FILE_FORK_CSRCFILE)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_SRCFILE)X \
                 $(CONFIG_PK_INTERVAL_TREE_FILE_FORK_CSRCFILE)

$(PK_INTERVAL_TREE_FILE_FORK_DIR)/build: FORCE
	$(Q)$(call PK_INTERVAL_TREE_FILE_FORK_sh,$(srctree))
