################################################
#
# BiscuitOS-anonymous-interval-tree
#
################################################
PK_INTERVAL_TREE_ANON_NAME        := BiscuitOS-anonymous-interval-tree
PK_INTERVAL_TREE_ANON_VERSION     := $(CONFIG_PK_INTERVAL_TREE_ANON_VERSION)
PK_INTERVAL_TREE_ANON_SITE        := $(CONFIG_PK_INTERVAL_TREE_ANON_URL)
PK_INTERVAL_TREE_ANON_GITHUB      := $(CONFIG_PK_INTERVAL_TREE_ANON_GITHUB)
PK_INTERVAL_TREE_ANON_SUBNAME     :=
PK_INTERVAL_TREE_ANON_BUILDARCH   := $(CONFIG_PK_INTERVAL_TREE_ANON_BUILDARCH)
PK_INTERVAL_TREE_ANON_DIR         := $(srctree)/package/interval-tree/BiscuitOS-anonymous-interval-tree
PK_INTERVAL_TREE_ANON_PATCH       := $(PK_INTERVAL_TREE_ANON_DIR)/patch
PK_INTERVAL_TREE_ANON_BSBIT       := BiscuitOS-anonymous-interval-tree-$(PK_INTERVAL_TREE_ANON_VERSION).bsbit
PK_INTERVAL_TREE_ANON_OUT         :=
PK_INTERVAL_TREE_ANON_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_INTERVAL_TREE_ANON_TAR         := $(CONFIG_PK_INTERVAL_TREE_ANON_TAR)
PK_INTERVAL_TREE_ANON_PACKAGE     :=
PK_INTERVAL_TREE_ANON_CONFIG      := $(CONFIG_PK_INTERVAL_TREE_ANON_CONFIG)
SUB_TARGET                  += $(PK_INTERVAL_TREE_ANON_DIR)/build

PK_INTERVAL_TREE_ANON_sh = $(CONFIG_SHELL) $(PK_INTERVAL_TREE_ANON_SCRIPTS) \
                 $1X \
                 $(PK_INTERVAL_TREE_ANON_NAME)X \
                 $(PK_INTERVAL_TREE_ANON_VERSION)X \
                 $(PK_INTERVAL_TREE_ANON_SITE)X \
                 $(PK_INTERVAL_TREE_ANON_GITHUB)X \
                 $(PK_INTERVAL_TREE_ANON_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_INTERVAL_TREE_ANON_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_INTERVAL_TREE_ANON_SUBNAME)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_CONFIG)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_LIBPATH)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_CPPFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_DPKCONFIG)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_CFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_LDFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_CXXFLAGS)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_ASFLAGS)X \
                 $(PK_INTERVAL_TREE_ANON_BSBIT)X \
                 $(PK_INTERVAL_TREE_ANON_BUILDARCH)X \
		 $(PK_INTERVAL_TREE_ANON_CSRCFILE)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_SRCFILE)X \
                 $(CONFIG_PK_INTERVAL_TREE_ANON_CSRCFILE)

$(PK_INTERVAL_TREE_ANON_DIR)/build: FORCE
	$(Q)$(call PK_INTERVAL_TREE_ANON_sh,$(srctree))
