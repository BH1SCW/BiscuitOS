PK_SWAP_MKSWAP_SWAPON_NAME        := BiscuitOS-mkswap-swapon
PK_SWAP_MKSWAP_SWAPON_VERSION     := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_VERSION)
PK_SWAP_MKSWAP_SWAPON_SITE        := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_URL)
PK_SWAP_MKSWAP_SWAPON_GITHUB      := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_GITHUB)
PK_SWAP_MKSWAP_SWAPON_SUBNAME     :=
PK_SWAP_MKSWAP_SWAPON_BUILDARCH   := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_BUILDARCH)
PK_SWAP_MKSWAP_SWAPON_DIR         := $(srctree)/package/SWAP/BiscuitOS-mkswap-swapon
PK_SWAP_MKSWAP_SWAPON_PATCH       := $(PK_SWAP_MKSWAP_SWAPON_DIR)/patch
PK_SWAP_MKSWAP_SWAPON_BSBIT       := BiscuitOS-mkswap-swapon-$(PK_SWAP_MKSWAP_SWAPON_VERSION).bsbit
PK_SWAP_MKSWAP_SWAPON_OUT         :=
PK_SWAP_MKSWAP_SWAPON_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SWAP_MKSWAP_SWAPON_TAR         := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_TAR)
PK_SWAP_MKSWAP_SWAPON_PACKAGE     :=
PK_SWAP_MKSWAP_SWAPON_CONFIG      := $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CONFIG)
SUB_TARGET                  += $(PK_SWAP_MKSWAP_SWAPON_DIR)/build

PK_SWAP_MKSWAP_SWAPON_sh = $(CONFIG_SHELL) $(PK_SWAP_MKSWAP_SWAPON_SCRIPTS) \
                 $1X \
                 $(PK_SWAP_MKSWAP_SWAPON_NAME)X \
                 $(PK_SWAP_MKSWAP_SWAPON_VERSION)X \
                 $(PK_SWAP_MKSWAP_SWAPON_SITE)X \
                 $(PK_SWAP_MKSWAP_SWAPON_GITHUB)X \
                 $(PK_SWAP_MKSWAP_SWAPON_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SWAP_MKSWAP_SWAPON_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SWAP_MKSWAP_SWAPON_SUBNAME)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CONFIG)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_LIBPATH)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CPPFLAGS)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_DPKCONFIG)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CFLAGS)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_LDFLAGS)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CXXFLAGS)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_ASFLAGS)X \
                 $(PK_SWAP_MKSWAP_SWAPON_BSBIT)X \
                 $(PK_SWAP_MKSWAP_SWAPON_BUILDARCH)X \
		 $(PK_SWAP_MKSWAP_SWAPON_CSRCFILE)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_SRCFILE)X \
                 $(CONFIG_PK_SWAP_MKSWAP_SWAPON_CSRCFILE)

$(PK_SWAP_MKSWAP_SWAPON_DIR)/build: FORCE
	$(Q)$(call PK_SWAP_MKSWAP_SWAPON_sh,$(srctree))
