################################################
#
# memblock_set_region_node
#
################################################
PK_MEMBLOCK_SET_REGION_NODE_NAME        := memblock_set_region_node
PK_MEMBLOCK_SET_REGION_NODE_VERSION     := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_VERSION)
PK_MEMBLOCK_SET_REGION_NODE_SITE        := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_URL)
PK_MEMBLOCK_SET_REGION_NODE_GITHUB      := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_GITHUB)
PK_MEMBLOCK_SET_REGION_NODE_SUBNAME     :=
PK_MEMBLOCK_SET_REGION_NODE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_BUILDARCH)
PK_MEMBLOCK_SET_REGION_NODE_DIR         := $(srctree)/package/memblock/memblock_set_region_node
PK_MEMBLOCK_SET_REGION_NODE_PATCH       := $(PK_MEMBLOCK_SET_REGION_NODE_DIR)/patch
PK_MEMBLOCK_SET_REGION_NODE_BSBIT       := memblock_set_region_node-$(PK_MEMBLOCK_SET_REGION_NODE_VERSION).bsbit
PK_MEMBLOCK_SET_REGION_NODE_OUT         :=
PK_MEMBLOCK_SET_REGION_NODE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_SET_REGION_NODE_TAR         := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_TAR)
PK_MEMBLOCK_SET_REGION_NODE_PACKAGE     :=
PK_MEMBLOCK_SET_REGION_NODE_CONFIG      := $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_SET_REGION_NODE_DIR)/build

PK_MEMBLOCK_SET_REGION_NODE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_SET_REGION_NODE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_NAME)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_VERSION)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_SITE)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_GITHUB)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_ASFLAGS)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_BSBIT)X \
                 $(PK_MEMBLOCK_SET_REGION_NODE_BUILDARCH)X \
		 $(PK_MEMBLOCK_SET_REGION_NODE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_SET_REGION_NODE_CSRCFILE)

$(PK_MEMBLOCK_SET_REGION_NODE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_SET_REGION_NODE_sh,$(srctree))
