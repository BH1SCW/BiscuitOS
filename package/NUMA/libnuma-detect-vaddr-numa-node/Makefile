PK_LIBNUMA_DETECT_NODE_NAME        := libnuma-detect-vaddr-numa-node
PK_LIBNUMA_DETECT_NODE_VERSION     := $(CONFIG_PK_LIBNUMA_DETECT_NODE_VERSION)
PK_LIBNUMA_DETECT_NODE_SITE        := $(CONFIG_PK_LIBNUMA_DETECT_NODE_URL)
PK_LIBNUMA_DETECT_NODE_GITHUB      := $(CONFIG_PK_LIBNUMA_DETECT_NODE_GITHUB)
PK_LIBNUMA_DETECT_NODE_SUBNAME     :=
PK_LIBNUMA_DETECT_NODE_BUILDARCH   := $(CONFIG_PK_LIBNUMA_DETECT_NODE_BUILDARCH)
PK_LIBNUMA_DETECT_NODE_DIR         := $(srctree)/package/NUMA/libnuma-detect-vaddr-numa-node
PK_LIBNUMA_DETECT_NODE_PATCH       := $(PK_LIBNUMA_DETECT_NODE_DIR)/patch
PK_LIBNUMA_DETECT_NODE_BSBIT       := libnuma-detect-vaddr-numa-node-$(PK_LIBNUMA_DETECT_NODE_VERSION).bsbit
PK_LIBNUMA_DETECT_NODE_OUT         :=
PK_LIBNUMA_DETECT_NODE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_LIBNUMA_DETECT_NODE_TAR         := $(CONFIG_PK_LIBNUMA_DETECT_NODE_TAR)
PK_LIBNUMA_DETECT_NODE_PACKAGE     :=
PK_LIBNUMA_DETECT_NODE_CONFIG      := $(CONFIG_PK_LIBNUMA_DETECT_NODE_CONFIG)
SUB_TARGET                  += $(PK_LIBNUMA_DETECT_NODE_DIR)/build

PK_LIBNUMA_DETECT_NODE_sh = $(CONFIG_SHELL) $(PK_LIBNUMA_DETECT_NODE_SCRIPTS) \
                 $1X \
                 $(PK_LIBNUMA_DETECT_NODE_NAME)X \
                 $(PK_LIBNUMA_DETECT_NODE_VERSION)X \
                 $(PK_LIBNUMA_DETECT_NODE_SITE)X \
                 $(PK_LIBNUMA_DETECT_NODE_GITHUB)X \
                 $(PK_LIBNUMA_DETECT_NODE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_LIBNUMA_DETECT_NODE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_LIBNUMA_DETECT_NODE_SUBNAME)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_CONFIG)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_LIBPATH)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_CPPFLAGS)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_DPKCONFIG)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_CFLAGS)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_LDFLAGS)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_CXXFLAGS)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_ASFLAGS)X \
                 $(PK_LIBNUMA_DETECT_NODE_BSBIT)X \
                 $(PK_LIBNUMA_DETECT_NODE_BUILDARCH)X \
		 $(PK_LIBNUMA_DETECT_NODE_CSRCFILE)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_SRCFILE)X \
                 $(CONFIG_PK_LIBNUMA_DETECT_NODE_CSRCFILE)

$(PK_LIBNUMA_DETECT_NODE_DIR)/build: FORCE
	$(Q)$(call PK_LIBNUMA_DETECT_NODE_sh,$(srctree))
